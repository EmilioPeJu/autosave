record(stringout, "$(P)$(C)Menu:currName") {
	field(VAL, "defaultConfig")
}
record(scalcout, "$(P)$(C)Menu:makeCurrName") {
	field(BB, "$(C)_")
	field(CALC, "BB+AA")
	field(OUT, "$(P)$(C)Menu:currName PP")
}
record(scalcout, "$(P)$(C)Menu:makeLoadName") {
	field(BB, "$(C)_")
	field(CALC, "BB+AA")
	field(OUT, "$(P)$(C)Menu:load.A PP")
}

# Should disable autosave trigger PV so it doesn't post its initial value?
record(longout, "$(P)$(C)Menu:trigger") {
	field(MDEL, "-1")
}

record(aSub,"$(P)$(C)Menu:load") {
	field(INAM, "configMenu_init")
	field(SNAM, "configMenu_do")
	field(FTA, "STRING")
	field(FTB, "INT")
}

record(stringout, "$(P)$(C)Menu:name1") {
}
record(sseq, "$(P)$(C)Menu:loadConfig1") {
	field(STR1, "working...")
	field(LNK1, "$(P)$(C)Menu:currName PP")
	field(DOL2, "$(P)$(C)Menu:name1")
	field(LNK2, "$(P)$(C)Menu:makeLoadName.AA PP")
}
record(stringout, "$(P)$(C)Menu:saveConfig1") {
	field(OMSL, "closed_loop")
	field(DOL, "$(P)$(C)Menu:name1")
	field(OUT, "$(P)$(C)Menu:makeCurrName.AA PP")
	field(FLNK, "$(P)$(C)Menu:trigger")
}

